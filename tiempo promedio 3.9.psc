Algoritmo tiempo_promedio
	escribir "indique tiempos obtenidos por dia";
	Escribir "lunes";
	leer lunes;
	Escribir "martes";
	leer martes;
	Escribir "miercoles";
	leer miercoles;
	escribir "jueves";
	leer jueves;
	Escribir "viernes";
	leer viernes;
	Escribir "sabado";
	leer sabado;
	tiempo <-(lunes+martes+miercoles+jueves+viernes+sabado)/6;
	escribir "su tiempo promedio es:";
	Escribir  tiempo, "segundos";
	
	
	
FinAlgoritmo
